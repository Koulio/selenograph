{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [2, "single"],
    "one-var": [2, "never"],
    "semi": [2, "always"],
    "react/jsx-no-undef": 2,
    "react/jsx-uses-vars": 2,
    "react/no-unknown-property": 2
  }
}